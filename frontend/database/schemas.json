{
  "tokens": {
    "schema": {
      "type": "object",
      "version": 0,
      "primaryKey": "address",
      "properties": {
        "id_number": { "type": "number" },
        "address": { "type": "string", "maxLength": 100 },
        "symbol": { "type": "string", "maxLength": 100 },
        "name": { "type": "string", "maxLength": 100 },
        "tokenName": { "type": "string", "maxLength": 100 },
        "tokenSymbol": { "type": "string", "maxLength": 100 },
        "decimal": { "type": "string", "maxLength": 100 },
        "shortDecimal": { "type": "string", "maxLength": 100 },
        "subAccounts": {
          "type": "array",
          "items": {
            "type": "record",
            "properties": {
              "numb": { "type": "string" },
              "name": { "type": "string" },
              "amount": { "type": "string" },
              "currency_amount": { "type": "string" }
            },
            "required": ["numb", "name", "amount", "currency_amount"]
          }
        },
        "fee": { "type": "string", "maxLength": 100 },
        "index": { "type": "string", "maxLength": 100 },
        "logo": { "type": "string", "maxLength": 100 },
        "supportedStandards": { "type": "array", "items": { "type": "string" } }
      },
      "required": [
        "id_number",
        "symbol",
        "name",
        "tokenName",
        "tokenSymbol",
        "decimal",
        "shortDecimal",
        "subAccounts",
        "fee",
        "supportedStandards"
      ],
      "indexes": ["name"]
    },
    "migrationStrategies": {}
  },
  "contacts": {
    "schema": {
      "type": "object",
      "version": 0,
      "primaryKey": "principal",
      "properties": {
        "principal": { "type": "string", "maxLength": 100 },
        "name": { "type": "string", "maxLength": 100 },
        "accountIdentier": { "type": "string", "maxLength": 100 },
        "assets": {
          "type": "array",
          "items": {
            "type": "record",
            "properties": {
              "symbol": { "type": "string" },
              "tokenSymbol": { "type": "string" },
              "logo": { "type": "string" },
              "subaccounts": {
                "type": "array",
                "items": {
                  "type": "record",
                  "properties": {
                    "name": { "type": "string" },
                    "subaccount_index": { "type": "string" },
                    "sub_account_id": { "type": "string" },
                    "allowance": {
                      "type": "record",
                      "properties": {
                        "allowance": { "type": "string" },
                        "expires_at": { "type": "string" }
                      }
                    }
                  },
                  "required": ["name", "subaccount_index", "sub_account_id"]
                }
              },
              "address": { "type": "string" },
              "decimal": { "type": "string" },
              "shortDecimal": { "type": "string" },
              "supportedStandards": { "type": "array", "items": { "type": "string" } }
            },
            "required": [
              "symbol",
              "tokenSymbol",
              "subaccounts",
              "address",
              "decimal",
              "shortDecimal",
              "supportedStandards"
            ]
          }
        }
      },
      "required": ["name", "assets"]
    },
    "migrationStrategies": {}
  }
}
